{
  "name": "sy-devs-cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.919.0",
    "@aws-sdk/lib-storage": "^3.919.0",
    "@nouance/payload-better-fields-plugin": "^2.0.2",
    "@payloadcms/db-mongodb": "^3.61.1",
    "@payloadcms/email-nodemailer": "^3.61.1",
    "@payloadcms/next": "^3.61.1",
    "@payloadcms/payload-cloud": "^3.61.1",
    "@payloadcms/plugin-form-builder": "^3.61.1",
    "@payloadcms/plugin-seo": "^3.61.1",
    "@payloadcms/richtext-lexical": "^3.61.1",
    "@payloadcms/storage-s3": "^3.61.1",
    "@payloadcms/ui": "^3.61.1",
    "@sentry/nextjs": "^9.46.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/styled-components": "^5.1.35",
    "aws-crt": "^1.27.5",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "graphql": "^16.11.0",
    "next": "15.3.2",
    "payload": "^3.61.1",
    "react": "19.1.0",
    "sharp": "0.32.6",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.19",
    "tmp": "^0.2.5"
  },
  "devDependencies": {
    "@payloadcms/plugin-cloud-storage": "^3.61.1",
    "@playwright/test": "1.50.0",
    "@testing-library/react": "16.3.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.18.13",
    "@types/nodemailer": "^6.4.21",
    "@types/pg": "^8.15.6",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "@types/tmp": "^0.2.6",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.3.0",
    "jsdom": "26.1.0",
    "mongodb": "^6.20.0",
    "mongodb-memory-server": "^10.2.3",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "playwright": "1.50.0",
    "playwright-core": "1.50.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
